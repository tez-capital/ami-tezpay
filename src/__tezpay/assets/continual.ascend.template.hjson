{
    executable: {{{ROOT_DIR}}}/bin/tezpay
    args: [
        --path={{{ROOT_DIR}}}
        continual
        {{#model.STARTUP_ARGS}}
        {{.}}
        {{/model.STARTUP_ARGS}}
    ]
    environment: {
        HOME: {{{ROOT_DIR}}}
    }
    working_directory: {{{ROOT_DIR}}}
    restart: always
    restart_delay: 1
    user: {{{USER}}}
}